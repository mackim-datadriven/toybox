{
  "name": "toybox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://mackim-datadriven.github.io/toybox",
  "repository": {
    "type": "git",
    "url": "https://github.com/mackim-datadriven/toybox.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "mkdir -p dist/standalone/physics_flashcards && cp dist/index.html dist/standalone/physics_flashcards/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test-production": "vite preview --base=/toybox/",
    "update-config": "node scripts/update-config.js",
    "validate-config": "node -e \"import('./scripts/config-loader.js').then(m => m.validateConfig())\"",
    "prebuild": "npm run validate-config",
    "setup": "echo 'Please edit github.config.json with your GitHub username and repository, then run npm run update-config'",
    "validate-setup": "node scripts/validate-setup.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
