    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.3.0",
    "glob": "^10.4.5",
    "postcss": "^8.4.39",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.4.14",
    "wrangler": "^4.13.2"
  },
  "overrides": {
    "glob": "^10.4.5",
    "rimraf": "^5.0.5",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@latest",
