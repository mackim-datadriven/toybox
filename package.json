    "lucide-react": "^0.408.0",
    "mermaid": "^11.5.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.59.0",
    "react-resizable-panels": "^3.0.3",
    "react-router-dom": "^7.1.2",
    "recharts": "^3.0.2",
    "sonner": "^2.0.5",
